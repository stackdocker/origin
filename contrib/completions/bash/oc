_oc_start-build()
{
    last_command="oc_start-build"
    commands=()

    flags=()
    two_word_flags=()
    flags_with_completion=()
    flags_completion=()

    flags+=("--build-loglevel=")
    flags+=("--commit=")
    flags+=("--env=")
    two_word_flags+=("-e")
    flags+=("--follow")
    flags+=("--from-build=")
    flags+=("--from-dir=")
    flags+=("--from-file=")
    flags+=("--from-repo=")
    flags+=("--from-webhook=")
    flags+=("--git-post-receive=")
    flags+=("--git-repository=")
    flags+=("--list-webhooks=")
    flags+=("--wait")
    flags+=("--api-version=")
    flags+=("--certificate-authority=")
